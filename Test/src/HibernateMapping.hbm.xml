<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="bh.w2optimize.entity.Component" table="component">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="name"></property>
		<property name="code"></property>
	</class>
	<class name="bh.w2optimize.entity.Element" table="element">
		<id name="id">
			<generator class="native"></generator>
		</id>
		<property name="componentCode"></property>
		<property name="name"></property>
		<property name="length"></property>
		<property name="width"></property>
	</class>
	<class name="bh.w2optimize.entity.GeneralComponent" table="generalcomponent">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="name"></property>
		<property name="code"></property>
		<property name="length"></property>
		<property name="width"></property>
		<property name="height"></property>
	</class>
	<class name="bh.w2optimize.entity.GeneralElement" table="generalelement">
		<id name="id">
			<generator class="native"></generator>
		</id>
		<property name="componentCode"></property>
		<property name="name"></property>
		<property name="length"></property>
		<property name="width"></property>
	</class>
	<class name="bh.w2optimize.entity.Accessory" table="accessory">
		<id name="id">
			<generator class="native"></generator>
		</id>
		<property name="code"></property>
		<property name="name"></property>
		<property name="price"></property>
	</class>
	<class name="bh.w2optimize.entity.WoodBoard" table="woodboard">
		<id name="id">
			<generator class="native"></generator>
		</id>
		<property name="code"></property>
		<property name="name"></property>
		<property name="material"></property>
		<property name="length"></property>
		<property name="width"></property>
		<property name="price"></property>
	</class>
</hibernate-mapping>