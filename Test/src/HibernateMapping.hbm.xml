		<property name="material" column="material"></property>
		<property name="length" column="length"></property>
		<property name="width" column="width"></property>
		<property name="price" column="price"></property>
	</class>
	<class name="bh.w2optimize.entity.WoodBoardPice" table="woodboardpice">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="code" column="code"></property>
		<property name="name" column="name"></property>
		<property name="material" column="material"></property>
		<property name="length" column="length"></property>
		<property name="width" column="width"></property>
		<property name="price" column="price"></property>
		<property name="number" column="number"></property>
	</class>
</hibernate-mapping>