<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="bh.w2optimize.entity.Component" table="component">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="name" column="name"></property>
		<property name="code" column="code"></property>
		<property name="elements" column="elements"></property>
	</class>
	<class name="bh.w2optimize.entity.GeneralComponent" table="generalcomponent">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="name" column="name"></property>
		<property name="code" column="code"></property>
		<property name="length" column="length"></property>
		<property name="width" column="width"></property>
		<property name="height" column="height"></property>
		<property name="elements" column="elements"></property>
	</class>
